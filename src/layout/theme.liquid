  <!-- MEYA Chatbot Integration -->
  <script type="text/javascript">
    $(document).ready(function() {
      var orbConfigOptions = {
        connectionOptions: {
          gridUrl: "https://grid.meya.ai",
          appId: "app-480903f657bc49538eacc2be6fccdbf6",
          integrationId: "integration.orb",
          connect: false,
          pageContext: {
            "customer_id": {{ customer.id | json }},
            "customer_email": {{ customer.email | json }},
            "customer_first_name": {{ customer.first_name | json }},
          },
        },
        theme: { "brandColor":"#202020" },
        windowApi: true,
      };

      window.orbConfig = orbConfigOptions;

      (function() {
        var script = document.createElement("script");
        script.type = "text/javascript";
        script.async = true;
        script.src = "https://cdn.meya.ai/v2/orb.js";
        document.body.appendChild(script);
        var fontStyleSheet = document.createElement("link");
        fontStyleSheet.rel = "stylesheet";
        fontStyleSheet.href = "https://cdn.meya.ai/font/inter.css";
        document.body.appendChild(fontStyleSheet);
      })();

      $(window).bind("beforeunload", function() {
        console.log(0);
        const closeEvent = orb.getEventStream().events
          .find(event => (event.type === "meya.session.event.chat.close"));

        if (!closeEvent) return;

        var lifecycleEvent = {
          "type": "meya.session.event.chat.close",
          "data": {
            "user_id": closeEvent.data.user_id,
            "thread_id": closeEvent.data.thread_id,
            "integration_id": closeEvent.data.integration_id
          }
        };

        orb.publishEvent(lifecycleEvent);

        return;
      })
    });
  </script>

  <script>
    function getUrlParameter(name) {
      name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
      var regex = new RegExp('[\\?&]' + name + '=([^&#]*)',  "i");
      var results = regex.exec(location.search);
      return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
    };
    function getCJCookie() {
      var name = "CJEVENT" + "=";
      var decodedCookie = decodeURIComponent(document.cookie);
      var ca = decodedCookie.split(';');
      for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          return c.substring(name.length, c.length);
        }
      }
      return "";
    }
    function SetCJCookie() {
      var url = window.location.search;
      if(url.match(/CJEVENT/i)){
        var d=new Date();
        d.setTime(d.getTime()+(45*24*60*60*1000));
        var expires = "; expires="+d.toGMTString();
        document.cookie =  "CJEVENT=" + getUrlParameter("CJEVENT") + expires + ';path=/';
      }

    }
    function eraseCookie(name) {
      document.cookie = name+'=; Max-Age=-99999999;';
    }
    eraseCookie('CJEVENT');
    SetCJCookie();
  </script>

